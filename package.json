{
  "name": "creevey",
  "version": "0.0.0",
  "bin": {
    "creevey": "./lib/cli.js"
  },
  "main": "lib/creevey.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:wKich/creevey.git",
  "license": "MIT",
  "scripts": {
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:tsc": "tsc --noEmit",
    "build": "npm-run-all --continue-on-error --parallel build:*",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:dts": "dts-generator --out ./lib/index.d.ts --project ./ --prefix creevey --types ../types/chai --types ../types/mocha --exclude \"types /*.d.ts\" --exclude \"node_modules/**/*.d.ts\""
  },
  "peerDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "selenium-webdriver": "^4.0.0-alpha.1"
  },
  "dependencies": {
    "koa": "^2.7.0",
    "mkdirp": "^0.5.1",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0",
    "uuid": "^3.3.2",
    "ws": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/koa": "^2.0.48",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.4",
    "@types/pixelmatch": "^4.0.0",
    "@types/pngjs": "^3.3.2",
    "@types/selenium-webdriver": "^3.0.15",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "dts-generator": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.3.4000"
  }
}
