{
  "name": "creevey",
  "version": "0.0.0",
  "bin": {
    "creevey": "./lib/cli.js"
  },
  "types": "lib/index.d.ts",
  "repository": "git@github.com:wKich/creevey.git",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:tsc": "tsc --noEmit",
    "start": "npm-run-all --parallel start:*",
    "start:client": "webpack-dev-server",
    "prebuild": "yarn clean",
    "build": "npm-run-all --continue-on-error --parallel build:*",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions .ts --ignore src/client",
    "build:client": "webpack --mode production",
    "build:dts": "dts-generator --out lib/index.d.ts --project ./ --prefix creevey --types ../types/chai --types ../types/mocha --exclude \"node_modules/**/*.d.ts\" --exclude \"./*.ts\" --exclude src/cli.ts --exclude \"src/client/**/*.tsx\""
  },
  "peerDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1"
  },
  "dependencies": {
    "@skbkontur/react-icons": "^3.1.6",
    "@skbkontur/react-ui": "^0.46.0",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0",
    "uuid": "^3.3.2",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/koa": "^2.0.48",
    "@types/koa-static": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.4",
    "@types/pixelmatch": "^4.0.0",
    "@types/pngjs": "^3.3.2",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/selenium-webdriver": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/ws": "^6.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "dts-generator": "^3.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
