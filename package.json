{
  "name": "creevey",
  "description": "creevey is a tool for automated visual testing, that tightly integrated with storybook",
  "version": "0.0.28",
  "bin": {
    "creevey": "./lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Dmitriy Lazarev <nflazzer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKich/creevey.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:tsc": "tsc --noEmit",
    "test": "npm-run-all --parallel test:*",
    "test:unit": "mocha --opts mocha.opts",
    "test:ui": "node --require ./scripts/babel-register -- src/cli",
    "start": "npm-run-all --parallel start:*",
    "start:client": "webpack-dev-server",
    "start:storybook": "start-storybook --ci -p 6006",
    "start:creevey": "wait-on -t 30000 http-get://localhost:6006/ && node --require ./scripts/babel-register -- src/cli --ui",
    "prebuild": "yarn clean && mkdir lib",
    "build": "npm-run-all --continue-on-error --parallel build:*",
    "build:types": "tsc --emitDeclarationOnly -p scripts/tsconfig.prod.json",
    "build:creevey": "babel src --out-dir lib --extensions .ts,.tsx --ignore src/client",
    "build:client": "webpack --mode production",
    "postbuild": "cp scripts/dist/* lib/",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "event-source-polyfill": "^1.0.9",
    "interpret": "^1.2.0",
    "koa": "^2.8.2",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pixelmatch": "^5.1.0",
    "pngjs": "^3.4.0",
    "uuid": "^3.3.3",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@emotion/core": "^10.0.21",
    "@skbkontur/react-icons": "^3.1.11",
    "@skbkontur/react-ui": "1.4.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@types/chai": "^4.2.3",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/interpret": "^1.1.1",
    "@types/koa": "^2.0.50",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/pixelmatch": "^5.0.0",
    "@types/pngjs": "^3.3.2",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/selenium-webdriver": "^4.0.3",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.39.2",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/ws": "^6.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rimraf": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
